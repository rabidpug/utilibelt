{
  "name": "utilibelt",
  "version": "1.1.0",
  "description": "a collection of utilities",
  "repository": "git+https://rabidpug@github.com/rabidpug/utilibelt.git",
  "author": "Matt Cuneo",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/index.min.js",
  "homepage": "https://github.com/rabidpug/utilibelt#readme",
  "bugs": {
    "url": "https://github.com/rabidpug/utilibelt/issues"
  },
  "keywords": [
    "redux",
    "actions",
    "methods"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-flow": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^23.2.0",
    "babel-loader": "^8.0.0-beta.4",
    "coveralls": "^3.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.13.0",
    "flow-bin": "^0.75.0",
    "flow-copy-source": "^2.0.0",
    "jest": "^23.2.0",
    "jest-junit": "^5.1.0",
    "prettier": "^1.13.6",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "webpack": "^4.12.1",
    "webpack-cli": "^3.0.8",
    "yarn-run-all": "^3.1.1"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "scripts": {
    "build": "run-s clean build:*",
    "clean": "rimraf dist lib es",
    "build:umd": "cross-env BABEL_ENV=es webpack",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel -d lib src --ignore '**/*.test.*'",
    "build:es": "cross-env BABEL_ENV=es babel -d es src --ignore '**/*.test.*'",
    "build:flowcjs": "flow-copy-source -v -i '**/*.test.*' src lib",
    "build:flowes": "flow-copy-source -v -i '**/*.test.*' src es",
    "test": "run-p test:*",
    "test:lint": "eslint \"src/**/*.js\"",
    "test:flow": "flow",
    "test:jest": "jest --coverage",
    "prepare": "run-s clean test build",
    "push": "./push.sh"
  },
  "sideEffects": false,
  "dependencies": {
    "chalk": "^2.4.1"
  }
}
